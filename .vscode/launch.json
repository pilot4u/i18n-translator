    {
      "type": "chrome",
      "request": "launch",
      "name": "Launch Chrome against localhost",
      "url": "http://localhost:9222/devtools/inspector.html?ws=localhost:9222/devtools/page/835c0bf2-8c7d-417e-a239-4bd20f8edec1",
      "webRoot": "${workspaceFolder}"
    }
  ]
}
